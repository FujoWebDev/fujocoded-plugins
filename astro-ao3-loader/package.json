{
  "name": "@fujocoded/astro-ao3-loader",
  "type": "module",
  "version": "0.0.3",
  "description": "Allows loading a series of AO3 works for the Astro Content Layer",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch src/",
    "validate": "npx publint"
  },
  "keywords": [
    "withastro",
    "astro-loader"
  ],
  "author": "FujoCoded LLC",
  "license": "MIT",
  "dependencies": {
    "@fujocoded/ao3.js": "^0.22.1",
    "@types/node": "^22.8.2",
    "astro": "^4.16.7",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
