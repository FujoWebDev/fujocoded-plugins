---
import Sportacus from "../assets/sportacus.small.png";
import Robbie from "../assets/robbie.small.png";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>Astro</title>
  </head>
  <body>
    <img src={Sportacus.src} alt='Sportacus from hit series "Lazytown"' />
    <!-- Do not trigger alt-text badge for empty alt -->
    <img src={Robbie.src} alt="" />
    <script>
      import "@fujocoded/astro-alt-text-toolkit";
    </script>
  </body>
</html>
