{
  "name": "@fujocoded/remark-capitalize-titles",
  "type": "module",
  "version": "0.0.1",
  "description": "Make titles consistent with remark",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./defaults": {
      "import": {
        "types": "./dist/capitalizations.d.ts",
        "default": "./dist/capitalizations.js"
      },
      "require": {
        "types": "./dist/capitalizations.d.cts",
        "default": "./dist/capitalizations.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:plugin && npm run build:capitalizations",
    "build:capitalizations": "tsup capitalizations.ts --format cjs,esm --dts",
    "build:plugin": "tsup index.ts --format cjs,esm --dts",
    "validate": " npx publint"
  },
  "keywords": [
    "remark"
  ],
  "author": "FujoCoded LLC",
  "license": "MIT",
  "dependencies": {
    "title": "^3.5.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/title": "^3.4.3",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "unified": "^11.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
