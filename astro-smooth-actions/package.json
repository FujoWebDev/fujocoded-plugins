{
  "name": "@fujocoded/astro-smooth-action",
  "type": "module",
  "version": "0.0.1",
  "description": "An Astro integration for smooth action handling",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "private": true,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
