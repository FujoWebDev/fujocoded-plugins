    "validate": " npx publint"
  },
  "keywords": [
    "withastro",
    "astro-integration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fujowebdev/fujocoded-plugins.git"
  },
  "author": "FujoCoded LLC",
  "license": "MIT",
  "devDependencies": {
    "astro": "^5.0.0",
    "tsdown": "^0.14.1"
  },
  "publishConfig": {
    "access": "public"
  }
