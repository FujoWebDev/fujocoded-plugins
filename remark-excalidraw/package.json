{
  "name": "@fujocoded/remark-excalidraw",
  "version": "0.0.1",
  "description": "Load excalidraw diagrams via remark",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "prefix": "./",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./component": {
      "import": {
        "types": "./dist/component.d.ts",
        "default": "./dist/component.js"
      },
      "require": {
        "types": "./dist/component.d.cts",
        "default": "./dist/component.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --clean",
    "build:component": "tsup component.tsx --format cjs,esm --dts --clean",
    "validate": " npx publint",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FujoWebDev/fujocoded-plugins.git"
  },
  "keywords": [
    "remark",
    "excalidraw"
  ],
  "author": "FujoCoded, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FujoWebDev/fujocoded-plugins/issues"
  },
  "homepage": "https://github.com/FujoWebDev/fujocoded-plugins#readme",
  "dependencies": {
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "remark": "^15.0.1",
    "remark-mdx": "^3.1.0",
    "tsup": "^8.3.6",
    "vitest": "^3.0.5"
  }
}
